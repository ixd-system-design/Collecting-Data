generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model cats {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  adoptionDate   DateTime? @db.Date
  birthDate      DateTime? @db.Date
  breed          String?
  declawed       Boolean?
  description    String?
  fosterDate     DateTime? @db.Date
  fosterDuration Int?
  location       String?
  name           String?
  neutered       Boolean?
  sex            String?
  primaryColor   String?
  secondaryColor String?
  playfulness    Int?
  appetite       Int?
  microchip      Int?
  isAdopted      Boolean?
  food           String?
}
